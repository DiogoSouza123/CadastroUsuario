<h1>Editar Usuario</h1>


<form action='/editarusuario' method="POST">
    <p>Login:</p>
    <input type="text" name="login" value={{usuario.dataValues.usuario}}>
    <p>Senha:</p>
    <input type="text" name="senha" value={{usuario.dataValues.senha}}><br>
    <input type="hidden" name="id" value={{usuario.dataValues.id}}><br>
    <button type="submit">Editar</button>
</form>


